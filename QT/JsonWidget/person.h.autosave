#ifndef PERSON_H
#define PERSON_H

#include<iostream>
#include<string>

using namespace std;

class person
{
public:
    person();
    
    void SetName(string value) {name = value;}
    string GetName() {return name;}
    void SetAge(int value) {age = value;}
    int GetAge()  {return age;}
    void SetPhone(string value) {phone = value;}
    string GetPhone() {return phone;}
    
    
private:
    string name;
    int age;
    string phone;
};

#endif // PERSON_H
